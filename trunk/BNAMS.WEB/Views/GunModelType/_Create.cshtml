

<div class="panel panel-primary dialog-panel">
    <div class="panel-heading">
        <h5>Gun Model Type Create</h5>
    </div>


    <input type="text" id="hdnGunModelTypeId" hidden="" value="0" />
    <input type="text" id="hdnSetupBy" hidden="" value="0" />
    <input type="text" id="hdnSetupDateTime" hidden="" value="0" />


    <div class="panel-body">
        <form class="form-horizontal" role="form" style="margin-left: 10px">
            <div class="row">


                <div class="form-group">
                    <label class="col-md-4 control-label">
                        Gun Model Type Code
                    </label>
                    <div class="col-md-4">
                        <input type="text" readonly="" id="txtGunModelTypeCode" class="form-control" placeholder="Capability Code(Auto)">
                    </div>
                </div>



                <div class="form-group required">
                    <label class="col-md-4 control-label">
                        Gun Model Type
                    </label>
                    <div class="col-md-8">
                        <input type="text" id="txtGunModelType" class="form-control" placeholder="Enter Gun Model Type">
                    </div>
                </div>


                <div class="form-group required">
                    <label class="col-md-4 control-label">
                        Short Name
                    </label>
                    <div class="col-md-8">
                        <input type="text" id="txtShortName" class="form-control" placeholder="Enter Short Name ">
                    </div>
                </div>
                
                
                <div class="form-group required">
                    <label class="col-md-4 control-label">
                        Create Date
                    </label>
                    <div class="col-md-8">
                        <input name="createDate" id="createDate" readonly="" data-date-format="dd.mm.yyyy" class="form-control form-control-inline input-medium date-picker" placeholder="Select Date" size="16" type="text" value="">
                    </div>
                </div>

                <div class="form-check">
                    <input type="checkbox" class="form-check-input" id="chkIsActive">
                    <label class="form-check-label">
                        Is Active
                    </label>
                </div>

            </div>

            <div class="form-actions right1">
                <button type="button" id="btnSubmit" class="btn btn-primary">
                    Save
                </button>
                <button type="button" id="btnCancel" class="btn btn-danger">
                    Reset
                </button>

            </div>


        </form>
    </div>
</div>
