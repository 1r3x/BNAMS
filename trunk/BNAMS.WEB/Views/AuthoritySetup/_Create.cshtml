

<div class="panel panel-primary dialog-panel">
    <div class="panel-heading">
        <h5>Authority Create</h5>
    </div>


    <input type="text" id="hdnAuthorityId" hidden="" value="0" />

    <div class="panel-body">
        <form class="form-horizontal" role="form" style="margin-left: 10px">
            <div class="row">


                <div class="form-group">
                    <label class="col-md-4 control-label">
                        Authority Code
                    </label>
                    <div class="col-md-4">
                        <input type="text" readonly="" id="txtAuthorityCode" class="form-control" placeholder="Authority Code(Auto)">
                    </div>
                </div>



                <div class="form-group required">
                    <label class="col-md-4 control-label">
                        Authority Name
                    </label>
                    <div class="col-md-8">
                        <input type="text" id="txtAuthorityName" class="form-control" placeholder="Enter Authority Name">
                    </div>
                </div>

                <div class="form-group">
                    <label class="col-md-4 control-label">
                        Area
                    </label>
                    <div class="col-md-8">
                        <select class="form-control" id="ddlArea">
                            <option></option>
                        </select>
                    </div>
                </div>


                <div class="form-group required">
                    <label class="col-md-4 control-label">
                        Contract
                    </label>
                    <div class="col-md-8">
                        <input type="text" id="txtAuthorityContract" class="form-control" placeholder="Enter Authority Contract">
                    </div>
                </div>

                <div class="form-group required">
                    <label class="col-md-4 control-label">
                        Email
                    </label>
                    <div class="col-md-8">
                        <input type="text" id="txtAuthorityEmail" class="form-control" placeholder="Enter Authority Email">
                    </div>
                </div>


                <div class="form-check">
                    <input type="checkbox" class="form-check-input" id="chkIsActive">
                    <label class="form-check-label">
                        Is Active
                    </label>
                </div>

            </div>

            <div class="form-actions right1">
                <button type="button" id="btnSubmit" class="btn btn-primary">
                    Save
                </button>
                <button type="button" id="btnCancel" class="btn btn-danger">
                    Reset
                </button>

            </div>


        </form>
    </div>
</div>

