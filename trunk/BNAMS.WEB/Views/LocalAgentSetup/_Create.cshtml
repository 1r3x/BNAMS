

<div class="panel panel-primary dialog-panel">
    <div class="panel-heading">
        <h5>Local Agent/Supplier Create</h5>
    </div>


    <input type="text" id="hdnLocalAgentId" hidden="" value="0" />
    <input type="text" id="hdnSetupBy" hidden="" value="0" />
    <input type="text" id="hdnSetupDateTime" hidden="" value="0" />


    <div class="panel-body">
        <form class="form-horizontal" role="form" style="margin-left: 10px">
            <div class="row">


                <div class="form-group">
                    <label class="col-md-4 control-label">
                        Local Agent Code
                    </label>
                    <div class="col-md-4">
                        <input type="text" readonly="" id="txtLocalAgentCode" class="form-control" placeholder="Agent Code(Auto)">
                    </div>
                </div>

                <div class="form-group">
                    <label class="col-md-4 control-label">
                        Agent Type
                    </label>
                    <div class="col-md-8">
                        <select class="form-control" id="ddlAgentType">
                            <option></option>
                        </select>
                    </div>
                </div>


                <div class="form-group required">
                    <label class="col-md-4 control-label">
                        Supplier Name
                    </label>
                    <div class="col-md-8">
                        <input type="text" id="txtSupplierName" class="form-control" placeholder="Enter Supplier Name">
                    </div>
                </div>


                <div class="form-group required">
                    <label class="col-md-4 control-label">
                        Address
                    </label>
                    <div class="col-md-8">
                        <input type="text" id="txtAddress" class="form-control" placeholder="Enter Address">
                    </div>
                </div>

                <div class="form-group">
                    <label class="col-md-4 control-label">
                        Enlistment Type
                    </label>
                    <div class="col-md-8">
                        <select class="form-control" id="ddlEnlistmentType">
                            <option></option>
                            
                        </select>
                    </div>
                </div>

                <div class="form-group required">
                    <label class="col-md-4 control-label">
                        Email
                    </label>
                    <div class="col-md-8">
                        <input type="text" id="txtEmail" class="form-control" placeholder="Enter Email">
                    </div>
                </div>

                <div class="form-group required">
                    <label class="col-md-4 control-label">
                        Contract Number
                    </label>
                    <div class="col-md-8">
                        <input type="text" id="txtContractNumber" class="form-control" placeholder="Enter Contract Number">
                    </div>
                </div>

                <div class="form-group">
                    <label class="col-md-4 control-label">
                        Country
                    </label>
                    <div class="col-md-8">
                        <select class="form-control" id="ddlCountry">
                            <option></option>
                        </select>
                    </div>
                </div>


                <div class="form-check">
                    <input type="checkbox" class="form-check-input" id="chkIsActive">
                    <label class="form-check-label">
                        Is Active
                    </label>
                </div>

            </div>

            <div class="form-actions right1">
                <button type="button" id="btnSubmit" class="btn btn-primary">
                    Save
                </button>
                <button type="button" id="btnCancel" class="btn btn-danger">
                    Reset
                </button>

            </div>


        </form>
    </div>
</div>
