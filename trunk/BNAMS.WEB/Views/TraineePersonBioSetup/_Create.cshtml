

<div class="panel panel-primary dialog-panel">
    <div class="panel-heading">
        <h5>Training Person Bio Data</h5>
    </div>


    <input type="text" id="hdnTraineePersonBioId" hidden="" value="0" />
    <input type="text" id="hdnSetupBy" hidden="" value="0" />
    <input type="text" id="hdnSetupDateTime" hidden="" value="0" />

    <div class="panel-body">
        <form class="form-horizontal" role="form" style="margin-left: 10px">
            <div class="row">


                <div class="form-group">
                    <label class="col-md-4 control-label">
                        Bio Data Code
                    </label>
                    <div class="col-md-4">
                        <input type="text" readonly="" id="txtBioDataCode" class="form-control" placeholder="Bio Data Code(Auto)">
                    </div>
                </div>



                <div class="form-group required">
                    <label class="col-md-4 control-label">
                        P.NO/O.NO
                    </label>
                    <div class="col-md-8">
                        <input type="text" id="txtPNo" class="form-control" placeholder="Enter P.NO/O.NO">
                    </div>
                </div>

                <div class="form-group">
                    <label class="col-md-4 control-label">
                        Rank
                    </label>
                    <div class="col-md-8">
                        <select class="form-control" id="ddlRank">
                            <option></option>
                        </select>
                    </div>
                </div>


                <div class="form-group required">
                    <label class="col-md-4 control-label">
                        Name
                    </label>
                    <div class="col-md-8">
                        <input type="text" id="txtName" class="form-control" placeholder="Enter Trainee Name">
                    </div>
                </div>

                

                <div class="form-check">
                    <input type="checkbox" class="form-check-input" id="chkIsActive">
                    <label class="form-check-label">
                        Is Active
                    </label>
                </div>

            </div>

            <div class="form-actions right1">
                <button type="button" id="btnSubmit" class="btn btn-primary">
                    Save
                </button>
                <button type="button" id="btnCancel" class="btn btn-danger">
                    Reset
                </button>

            </div>


        </form>
    </div>
</div>

