

<div class="panel panel-primary dialog-panel">
    <div class="panel-heading">
        <h5>Status After Maintaince</h5>
    </div>


    <input type="text" id="hdnStatusAfterMaintainceId" hidden="" value="0" />
    <input type="text" id="hdnSetupBy" hidden="" value="0" />
    <input type="text" id="hdnSetupDateTime" hidden="" value="0" />


    <div class="panel-body">
        <form class="form-horizontal" role="form" style="margin-left: 10px">
            <div class="row">

                <div class="form-group">
                    <label class="col-md-4 control-label">
                        Status After Maintaince Code
                    </label>
                    <div class="col-md-4">
                        <input type="text" readonly="" id="txtMaintainceCode" class="form-control" placeholder="Code (Auto)">
                    </div>
                </div>


                <div class="form-group">
                    <label class="col-md-4 control-label">
                        Registration No
                    </label>
                    <div class="col-md-8">
                        <select class="form-control" id="ddlRegistrationNo">
                            <option></option>
                        </select>
                    </div>
                </div>

                <div class="form-group required">
                    <label class="col-md-4 control-label">
                        Maintaince Result
                    </label>
                    <div class="col-md-8">
                        <textarea readonly="" id="txtMaintainceResult" class="form-control" placeholder="Maintaince Details" rows="4" cols="50"></textarea>
                    </div>
                </div>
                
                
                <div class="form-group">
                    <label class="col-md-4 control-label">
                        Maintaince Status
                    </label>
                    <div class="col-md-8">
                        <select class="form-control" id="ddlMaintainceStatus">
                            <option></option>
                        </select>
                    </div>
                </div>


                <div class="form-group required">
                    <label class="col-md-4 control-label">
                        Remarks
                    </label>
                    <div class="col-md-8">
                        <input type="text" id="txtRemarks" class="form-control" placeholder="Enter Remarks">
                    </div>
                </div>

            </div>

            <div class="form-actions right1">
                <button type="button" id="btnSubmit" class="btn btn-primary">
                    Save
                </button>
                <button type="button" id="btnCancel" class="btn btn-danger">
                    Reset
                </button>

            </div>


        </form>
    </div>
</div>
