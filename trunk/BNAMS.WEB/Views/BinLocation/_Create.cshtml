

<div class="panel panel-primary dialog-panel">
    <div class="panel-heading">
        <h5>Bin Location Create</h5>
    </div>


    <input type="text" id="hdnBinLocationId" hidden="" value="0" />
    <input type="text" id="hdnSetupBy" hidden="" value="0" />
    <input type="text" id="hdnSetupDateTime" hidden="" value="0" />


    <div class="panel-body">
        <form class="form-horizontal" role="form" style="margin-left: 10px">
            <div class="row">

                <div class="form-group">
                    <label class="col-md-4 control-label">
                        Program Name
                    </label>
                    <div class="col-md-4">
                        <input type="text" id="txtProgramName" class="form-control" placeholder="Program Name">
                    </div>
                </div>

                <div class="form-group required">
                    <label class="col-md-4 control-label">
                        Process Date
                    </label>
                    <div class="col-md-8">
                        <input readonly="" data-date-format="dd/mm/yyyy" class="form-control form-control-inline input-medium date-picker" id="dateProcess" placeholder="Select Date" size="16" type="text" value="">
                    </div>
                </div>

                <div class="form-group required">
                    <label class="col-md-4 control-label">
                        Warehouse Code
                    </label>
                    <div class="col-md-8">
                        <input type="text" id="txtWarehouseCode" class="form-control" placeholder="Enter Warhouse Code">
                        
                    </div>
                </div>


                <div class="form-group required">
                    <label class="col-md-4 control-label">
                        Warehouse Name
                    </label>
                    <div class="col-md-8">
                        <input type="text" readonly="" id="txtWarehouseName" class="form-control" placeholder="Warhouse Name(Auto)">
                        <input type="text" id="hdnWareHouseId" hidden="" value="0" />
                    </div>
                </div>



                <div class="form-group required">
                    <label class="col-md-4 control-label">
                        Shelf ID
                    </label>
                    <div class="col-md-8">
                        <input type="text" id="txtSehlfId" class="form-control" placeholder="Enter Shelf ID">
                    </div>
                </div>

                <div class="form-group required">
                    <label class="col-md-4 control-label">
                        Row ID
                    </label>
                    <div class="col-md-8">
                        <input type="text" id="txtRowId" class="form-control" placeholder="Enter Address 2">
                    </div>
                </div>


                <div class="form-group">
                    <label class="col-md-4 control-label">
                        Product Category
                    </label>
                    <div class="col-md-8">
                        <select class="form-control" id="ddlProductCategory">
                            <option></option>
                        </select>
                    </div>
                </div>

                <div class="form-check">
                    <input type="checkbox" class="form-check-input" id="chkIsActive">
                    <label class="form-check-label">
                        Is Active
                    </label>
                </div>

            </div>

            <div class="form-actions right1">
                <button type="button" id="btnSubmit" class="btn btn-primary">
                    Save
                </button>
                <button type="button" id="btnCancel" class="btn btn-danger">
                    Reset
                </button>

            </div>


        </form>
    </div>
</div>
