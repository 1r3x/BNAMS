

<div class="panel panel-primary dialog-panel">
    <div class="panel-heading">
        <h5> Issue and Receive Item</h5>
    </div>


    <input type="text" id="hdnIndentId" hidden="" value="0" />
    <input type="text" id="hdnSetupBy" hidden="" value="0" />
    <input type="text" id="hdnSetupDateTime" hidden="" value="0" />


    <div class="panel-body">
        <form class="form-horizontal" role="form" style="margin-left: 10px">
            <div class="row">


              

                <div class="form-group">
                    <label class="col-md-4 control-label">
                        Indent No
                    </label>
                    <div class="col-md-4">
                        <input type="text" readonly="" id="txtIndentNo" class="form-control" placeholder="Indent No (Auto)">
                    </div>
                </div>

                <div class="form-group">
                    <label class="col-md-4 control-label">
                        Indent From
                    </label>
                    <div class="col-md-8">
                        <select class="form-control" id="ddlIndentFrom">
                            <option></option>
                        </select>
                    </div>
                </div>


                <div class="form-group">
                    <label class="col-md-4 control-label">
                        Issue Person
                    </label>
                    <div class="col-md-8">
                        <select class="form-control" id="ddlIssuePerson">
                            <option></option>
                        </select>
                    </div>
                </div>


                <div class="form-group">
                    <label class="col-md-4 control-label">
                        Item Code
                    </label>
                    <div class="col-md-8">
                        <select class="form-control" id="ddlItemCode">
                            <option></option>
                        </select>
                    </div>
                </div>

                <div class="form-group">
                    <label class="col-md-4 control-label">
                        Item Name
                    </label>
                    <div class="col-md-8">
                        <input type="text" readonly="" id="txtItemName" class="form-control" placeholder="Item Name">
                    </div>
                </div>



                <div class="form-group">
                    <label class="col-md-4 control-label">
                        Composite Code
                    </label>
                    <div class="col-md-8">
                        <select class="form-control" id="ddlCompositeCode">
                            <option></option>
                        </select>
                    </div>
                </div>

                <div class="form-group">
                    <label class="col-md-4 control-label">
                        Composite Name
                    </label>
                    <div class="col-md-8">
                        <input type="text" readonly="" id="txtCompositeName" class="form-control" placeholder="Composite Name">
                    </div>
                </div>



                <div class="form-group">
                    <label class="col-md-4 control-label">
                        Product Supplier Name
                    </label>
                    <div class="col-md-8">
                        <input type="text" readonly="" id="txtProductSupplierName" class="form-control" placeholder="Product Supplier Name">
                    </div>
                </div>



                <div id="txtIdentQuantiryDiv" class="form-group required">
                    <label class="col-md-4 control-label">
                        Ident Quantity
                    </label>
                    <div class="col-md-4">
                        <input type="text" id="txtIdentQuantiry" class="form-control" placeholder="Ident Quantity">
                    </div>
                </div>



                <div class="form-group required">
                    <label class="col-md-4 control-label">
                        Indent Validity Date
                    </label>
                    <div class="col-md-8">
                        <input id="dateOfIndentValidity" readonly="" data-date-format="dd/mm/yyyy" class="form-control form-control-inline input-medium date-picker" placeholder="Select Date" size="16" type="text" value="">
                    </div>
                </div>


                <div class="form-group required">
                    <label class="col-md-4 control-label">
                        Received Date
                    </label>
                    <div class="col-md-8">
                        <input id="dateOfReceived" readonly="" data-date-format="dd/mm/yyyy" class="form-control form-control-inline input-medium date-picker" placeholder="Select Date" size="16" type="text" value="">
                    </div>
                </div>




                <div class="form-group">
                    <label class="col-md-4 control-label">
                        Other Options
                    </label>
                    <div class="col-md-8">
                        <input type="text" readonly="" id="txtOtherOptions" class="form-control" placeholder="Other Options">
                    </div>
                </div>




                <div class="form-group">
                    <label class="col-md-4 control-label">
                        Remarks
                    </label>
                    <div class="col-md-8">
                        <input type="text" id="txtRemarks" class="form-control" placeholder="Remarks">
                    </div>
                </div>



            </div>

            <div class="form-actions right1">
                <button type="button" id="btnSubmit" class="btn btn-primary">
                    Receive
                </button>
                <button type="button" id="btnAbort" class="btn btn-danger">
                    Cancel
                </button>
                @*<button type="button" id="btnCancel" class="btn btn-danger">
                    Reset
                </button>*@

            </div>


        </form>
    </div>
</div>
